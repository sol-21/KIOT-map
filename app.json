{
	"expo": {
		"name": "Kiot Map",
		"slug": "Kiot-Map",
		"version": "1.0.0",
		"description": "this is kombolcha institute of technology map that help students to discovere locations",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/kiot-splash-screen.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"plugins": [
			[
				"expo-location",
				{
					"locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
				}
			]
		],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "io.expo.client"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.sol_17.KiotMap"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"extra": {
			"eas": {
				"projectId": "17e310f7-78cc-467d-ba72-6ed0bb70b94d"
			}
		}
	}
}
